{
  "expo": {
    "name": "Todoリスト",
    "slug": "react-native-todo-list",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/icon.png",
    "splash": {
      "image": "./assets/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#167476"
    },
    "updates": {
      "fallbackToCacheTimeout": 0
    },
    "assetBundlePatterns": ["**/*"],
    "ios": {
      "buildNumber": "1.0.0",
      "bundleIdentifier": "com.username.react.native.todo.list",
      "supportsTablet": false
    },
    "android": {
      "versionCode": 1,
      "package": "com.username.todo.list",
      "icon": "./assets/android_icon.png"
    },
    "web": {
      "favicon": "./assets/favicon.png"
    }
  }
}
